{% if content is image %}
    {% include '@Layout/Widget/Image/base.html.twig' with {'image': content} %}
{% elseif content is video %}
    {% include '@Layout/Widget/Video/Minimize/base.html.twig' with {'video': content} %}
{% elseif content is youtube_link %}
    {% include '@Layout/Widget/YoutubeLink/Minimize/base.html.twig' with {'youtube_link': content} %}
{% endif %}